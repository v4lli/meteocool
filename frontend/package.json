{
  "name": "meteocool",
  "version": "0.0.1",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode production",
    "build-dev": "webpack --config webpack.dev.js --mode development",
    "start": "webpack-dev-server --config webpack.config.js --mode development --content-base dist",
    "start-dev": "webpack-dev-server --config webpack.config.js --mode development --content-base dist --disable-host-check --host 0.0.0.0",
    "start-prod": "webpack-dev-server --config webpack.config.js --mode production --content-base dist --disable-host-check --host 0.0.0.0",
    "serve-release": "npm run build && http-server -g -c0 dist",
    "lint": "eslint src/*.js"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "ol": "^5.1.3",
    "popper.js": "^1.14.4",
    "rbush": "^2.0.2",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.2",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.4.2",
    "postcss-loader": "^3.0.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "workbox-webpack-plugin": "^3.4.1"
  }
}
