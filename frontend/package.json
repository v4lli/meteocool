    "socket.io-client": "^2.1.1",
    "workbox-window": "^4.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
